{
  "name": "@joshmccall/demo",
  "description": "demo",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "repository": "joshmccall221/demo",
  "bugs": "https://github.com/joshmccall221/demo/issues",
  "homepage": ".",
  "author": "Josh McCall <joshmccall221@gmail.com> (joshuamccall.com)",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all --print-label --parallel build:*",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "eject": "react-scripts eject",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha --recursive test/unit",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "storybook:build-surge": "npm run storybook:build;cd storybook-static; echo '\r' |  surge --domain cela-demo-storybook.surge.sh"
  },
  "dependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.4",
    "corber": "^1.3.10",
    "corber-webpack-plugin": "^1.0.0",
    "office-ui-fabric-react": "^6.164.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@travi/any": "^1.8.5",
    "@travi/babel-preset": "^3.0.0",
    "@travi/eslint-config-travi": "^1.8.0",
    "@types/storybook__react": "^4.0.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "commitlint-config-travi": "^1.2.40",
    "cucumber": "^5.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-travi": "^1.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.8.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^7.3.1",
    "travis-lint": "^1.0.0",
    "typescript": "^3.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
